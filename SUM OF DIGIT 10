import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner k = new Scanner(System.in);
        int kav = k.nextInt();
        int um = 0;
        if(kav<=99 ){
            System.out.println("Invalid Input");
        }else {

        while (kav > 0) {
            int digit = kav % 10;
            um += digit;
            kav /= 10;
        }
        
        System.out.println("Sum of digit is " + um);
        }
    }}
    
